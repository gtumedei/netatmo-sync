{
  "name": "netatmo-sync",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "dev": "cross-env NODE_ENV=development tsx src/main.ts",
    "build": "esbuild src/main.ts --bundle --platform=node --format=esm --packages=external --outfile=dist/main.js",
    "typecheck": "tsc --noEmit",
    "db": "drizzle-kit",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "^0.15.9",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.44.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
